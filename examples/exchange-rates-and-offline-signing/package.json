{
  "name": "lnurl-server-with-exchange-rates-and-offline-signing",
  "version": "1.0.0",
  "description": "Example lnurl-server with exchanges-rates and offline signing",
  "main": "server.js",
  "private": true,
  "scripts": {
    "generate-api-key": "./node_modules/.bin/lnurl generateApiKey --encoding 'base64'",
    "generate-signed-lnurl": "node ./scripts/generate-signed-lnurl.js",
    "lnurl-encode": "./node_modules/.bin/lnurl encode",
    "server:debug": "DEBUG=lnurl* node server.js",
    "start": "npm run server:debug",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chill117/lnurl-node.git"
  },
  "author": {
    "name": "Charles Hill",
    "email": "chill@degreesofzero.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chill117/lnurl-node/issues"
  },
  "homepage": "https://github.com/chill117/lnurl-node",
  "dependencies": {
    "bignumber.js": "9.0.0",
    "lnurl": "0.3.1"
  }
}
